	Loose Coupling:
	String param = "vector";
	
	List lst = ListProvider.getList(param);
	
	Collection col = CollectionProvider.getCollection("priority");
	
	//lst = new LinkedList();
	
	//lst = new Vector();
	
	
	
	class ListProvider {
	
		public static getList(String param) {
			if(param.equals("array"))
				return new ArrayList();
			else if(param.equals("list");
				return new LinkedList();
			else if(param.equals("vector");
				return new Vector();
		}
	}
	
	Associative Array (Key-value pair)
