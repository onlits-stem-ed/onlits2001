		class Student
		{
			$this->name = null;

			__contruct(string $name)
			{
				$this->name = $name;
			}
			
			setName(string $name)
			{
				$name = $name;
			}
			
			getName()
			{
				return $this->name;
			}
		}
		
		$student = new Student("Santosh");
		
		$student->setName("Raju");
		
		echo $student->getName();
		
